<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Homepage</title>

  <!-- bootstrap -->


  <!-- Google font -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet" />
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />

  <!-- CSS -->
  <link rel="stylesheet" href="/css/homepagestyle.css" />
  <script src="/js/scripthomepage.js">
  </script>
  <!-- </head>
  <header><%- include('partials/header'); %></header> -->

<body>
  <%- include('partials/header'); %>
    <section id="section-1">
      <div class="container-fluid">
        <div class="row">
          <div class="col"></div>
          <div class="col justify-content-center">
            <form>
              <div class="search">
                <span class="search-icon material-symbols-outlined">search</span>
                <input class="search-input" type="search" placeholder="Search" />
              </div>
            </form>
          </div>
          <div class="col"></div>
        </div>
      </div>
    </section>

    <section id="section-2">
      <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col-md-9">
            <div id="carouselExampleSlidesOnly" class="carousel slide" data-bs-ride="carousel">
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="./pic/Screenshot 2024-02-28 054734.png" class="d-block w-100" alt="pic1" />
                </div>
                <div class="carousel-item">
                  <img src="./pic/Screenshot 2024-02-28 054842.png" class="d-block w-100" alt="pic2" />
                </div>
                <div class="carousel-item">
                  <img src="./pic/Screenshot 2024-02-28 054734.png" class="d-block w-100" alt="pic3" />
                </div>
                <div class="carousel-item">
                  <img src="./pic/Screenshot 2024-02-28 054842.png" class="d-block w-100" alt="pic4" />
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <section id="section-3">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <h5>New Arrivals</h5>
          </div>
        </div>
      </div>
    </section>

    <section id="section-4">
      <div class="container">
        <div class="slider-wrapper">
          <button id="prev-slide" class="slide-button material-symbols-outlined">
            chevron_left
          </button>
          <div class="image-list">
            <a href="/product_1"> <img src="./pic/pink_shoe.jpg" alt="new1" class="image-item" />
            </a>
            <a href="/product_2"> <img src="./pic/shirt.jpg" alt="new2" class="image-item" /> </a>
            <a href="/product_1"> <img src="./pic/pink_shoe.jpg" alt="new3" class="image-item" /> </a>
            <a href="/product_2"><img src="./pic/shirt.jpg" alt="new4" class="image-item" /></a>
            <a href="/product_1"><img src="./pic/pink_shoe.jpg" alt="new5" class="image-item" /></a>
            <a href="/product_2"><img src="./pic/shirt.jpg" alt="new6" class="image-item" /></a>
          </div>
          <button id="next-slide" class="slide-button material-symbols-outlined">
            chevron_right
          </button>
        </div>
        <div class="slider-scrollbar">
          <div class="scrollbar-track">
            <div class="scrollbar-thumb"></div>
          </div>
        </div>
      </div>
    </section>

    <section id="section-5">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <h5>Best Seller</h5>
          </div>
        </div>
      </div>
    </section>

    <section id="section-6">
      <div class="container">
        <div class="slider-wrapper-2">
          <button id="prev-slide-2" class="slide-button-2 material-symbols-outlined">
            chevron_left
          </button>
          <div class="image-list-2">
            <a href="/product_2"></a><img src="./pic/shirt.jpg" alt="best1" class="image-item-2" /></a>
            <a href="/product_1"></a><img src="./pic/pink_shoe.jpg" alt="best2" class="image-item-2" /></a>
            <a href="/product_2"></a><img src="./pic/shirt.jpg" alt="best3" class="image-item-2" /></a>
            <a href="/product_1"></a><img src="./pic/pink_shoe.jpg" alt="best4" class="image-item-2" /></a>
            <a href="/product_2"></a><img src="./pic/shirt.jpg" alt="best5" class="image-item-2" /></a>
            <!-- <a href="/product_1"></a><img src="./pic/pink_shoe.jpg" alt="best6" class="image-item-2" /></a> -->
          </div>
          <button id="next-slide-2" class="slide-button-2 material-symbols-outlined">
            chevron_right
          </button>
        </div>
        <div class="slider-scrollbar-2">
          <div class="scrollbar-track-2">
            <div class="scrollbar-thumb-2"></div>
          </div>
        </div>
      </div>
    </section>

    <section id="section-7">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-12">
            <h5>Promotion</h5>
          </div>
        </div>
      </div>
    </section>

    <section id="section-8">
      <div class="container-fluid">
        <div class="row">
          <div class="col-md-6">
            <img src="./pic/promo_1.png" alt="promo2" class="image-promo" />
          </div>
          <div class="col-md-6">
            <img src="./pic/promo_2.png" alt="promo3" class="image-promo" />
          </div>
          <div class="col-md-6">
            <img src="./pic/promo_1.png" alt="promo2" class="image-promo" />
          </div>
          <div class="col-md-6">
            <img src="./pic/promo_2.png" alt="promo3" class="image-promo" />
          </div>
        </div>
      </div>
    </section>



    <script>
      const myCarouselElement = document.querySelector("#myCarousel");

      const carousel = new bootstrap.Carousel(myCarouselElement, {
        interval: 2000,
        touch: false,
      });
    </script>
</body>
<footer><%- include('partials/footer'); %></footer>

</html>